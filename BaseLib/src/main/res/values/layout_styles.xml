    <style name="tv_user_gray">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/appGray</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <style name="tv_left" parent="tv_user_gray">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/dp_16</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>

    <style name="tv_right" parent="tv_user_gray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/dp_8</item>
        <item name="android:layout_marginRight">@dimen/dp_16</item>
        <item name="android:gravity">right|center_vertical</item>
    </style>

    <style name="btn_" parent="wrap_wrap">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:layout_marginLeft">@dimen/dp_16</item>
        <item name="android:layout_marginRight">@dimen/dp_16</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:text">提交</item>
        <item name="android:background">@drawable/btn_bg_rect_app_green</item>
    </style>

    <style name="btn_submit" parent="match_wrap">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:layout_marginTop">@dimen/dp_40</item>
